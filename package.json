{
  "name": "xact-frontend-app",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start-local": "ng serve --configuration=developer",
    "start-dev": "ng serve --configuration=development",
    "build-local": "ng lint && ng build --configuration=developer",
    "build-dev": "ng lint && ng build --configuration=development",
    "build-qa": "ng build --configuration=qa",
    "build-prod": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "jest --testPathPattern=src/",
    "test:coverage": "jest --coverage --testPathPattern=src/  || exit 0",
    "e2e": "cypress run",
    "lint": "ng lint",
    "updateBuild": "node ./replace.build.js",
    "prepare": "husky install",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.6",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.6",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@badeball/cypress-cucumber-preprocessor": "^10.0.2",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/entity": "^13.2.0",
    "@ngrx/router-store": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@okta/okta-angular": "^5.1.1",
    "@okta/okta-auth-js": "^6.3.0",
    "@types/file-saver": "^2.0.5",
    "angular-bootstrap-md": "^13.0.0",
    "date-fns": "^2.28.0",
    "dependency": "^0.0.1",
    "dotenv": "^16.0.0",
    "file-saver": "^2.0.5",
    "jest-createspyobj": "^2.0.0",
    "launch": "^0.0.1",
    "lodash-es": "^4.17.21",
    "mdb-ui-kit": "^4.0.0",
    "moment": "^2.29.4",
    "ng-http-loader": "^11.0.1",
    "ng-simple-state": "^13.0.6",
    "ngrx-store-freeze": "^0.2.4",
    "ngrx-store-localstorage": "^13.0.1",
    "request": "^2.79.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "~13.2.6",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.2.6",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/localize": "^13.2.6",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "cypress": "^9.5.2",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.0.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "karma-coverage": "~2.1.0",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.2",
    "mochawesome-merge": "^4.2.1",
    "replace-in-file": "^6.3.2",
    "sonar-scanner": "^3.1.0",
    "typescript": "~4.5.2"
  },
  "jest": {
    "roots": [
      "<rootDir>",
      "src/app"
    ],
    "modulePaths": [
      "<rootDir>",
      "src/app"
    ],
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    },
    "cypress-cucumber-preprocessor": {
      "nonGlobalStepDefinitions": true
    },
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    }
  }
}
